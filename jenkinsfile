pipeline{
    agent any
    stages{
        stage('check-out'){
            steps{
                git"https://github.com/govind0707/my-maven-project.git"
            }
        }
        stage('clean'){
            steps{
                sh"mvn clean"
            }
        }
        stage('build'){
            steps{
                sh"mvn compile"
            }
        stage('test'){
            steps{
                sh"mvn test"
            }
        }
        }
        stage('package'){
            steps{
                sh"mvn package"
            }
        }
    }
}
